{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,ts,tsx,yml}\" --write",
		"format:check": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,ts,tsx,yml}\" --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run"
	},
	"dependencies": {
		"compression": "^1.7.1",
		"glob": "^7.1.4",
		"polka": "next",
		"sirv": "^0.4.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@sapper/app": "link:./src/node_modules/@sapper/app",
		"@sapper/server": "link:./src/node_modules/@sapper/server",
		"@sapper/service-worker": "link:./src/node_modules/@sapper/service-worker",
		"focus-visible": "^5.0.2",
		"front-matter": "^3.0.2",
		"highlight.js": "^9.15.10",
		"mdsvex": "^0.4.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.18.2",
		"rollup": "^1.12.0",
		"rollup-plugin-babel": "^4.0.2",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-pnp-resolve": "^1.1.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^5.0.1",
		"rollup-plugin-terser": "^4.0.4",
		"sapper": "^0.27.9",
		"svelte": "^3.12.1"
	},
	"prettier": {
		"semi": true,
		"singleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	}
}
