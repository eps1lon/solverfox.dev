{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"packageManager": "yarn@4.4.0",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier . --ignore-path ./.eslintignore --write",
		"format:check": "prettier . --ignore-path ./.eslintignore --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"test:types": "tsc --project ./tsconfig.json",
		"test:types:svelte": "svelte-check --tsconfig ./src/tsconfig.json"
	},
	"dependencies": {
		"@actions/core": "1.11.1",
		"@actions/github": "6.0.0",
		"@octokit/graphql": "7.1.0",
		"compression": "1.7.4",
		"glob": "11.0.0",
		"polka": "1.0.0-next.25",
		"sirv": "1.0.19",
		"svelte-check": "^3.5.2",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@babel/core": "7.25.2",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.24.7",
		"@babel/preset-env": "7.25.3",
		"@babel/runtime": "7.25.0",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-typescript": "^11.1.4",
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^20.5.9",
		"focus-visible": "5.2.0",
		"front-matter": "4.0.2",
		"highlight.js": "11.10.0",
		"mdsvex": "0.11.2",
		"npm-run-all2": "6.2.2",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.6",
		"rollup": "4.19.1",
		"rollup-plugin-svelte": "7.2.2",
		"sapper": "0.29.3",
		"svelte": "3.59.2",
		"svelte-preprocess": "^6.0.0",
		"tslib": "^2.6.2",
		"typescript": "5.5.4"
	}
}
