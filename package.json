{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,svelte,ts,tsx,yml}\" --write",
		"format:check": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,ts,tsx,yml}\" --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run"
	},
	"dependencies": {
		"@actions/core": "^1.2.6",
		"@actions/github": "^4.0.0",
		"@octokit/graphql": "^4.5.9",
		"compression": "^1.7.1",
		"glob": "^7.1.6",
		"polka": "1.0.0-next.11",
		"sirv": "^1.0.11",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.13.8",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.13.15",
		"@babel/preset-env": "^7.13.15",
		"@babel/runtime": "^7.13.17",
		"focus-visible": "^5.2.0",
		"front-matter": "^3.0.2",
		"highlight.js": "^10.6.0",
		"mdsvex": "^0.8.9",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.1.6",
		"rollup": "^2.35.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"sapper": "^0.29.1",
		"svelte": "^3.34.0"
	}
}
