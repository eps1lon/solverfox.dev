{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,svelte,ts,tsx,yml}\" --write",
		"format:check": "prettier --ignore-path ./.eslintignore \"**/*.{js,jsx,json,ts,tsx,yml}\" --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run"
	},
	"dependencies": {
		"@actions/core": "^1.2.6",
		"@actions/github": "^2.2.0",
		"@octokit/graphql": "^4.5.6",
		"compression": "^1.7.1",
		"glob": "^7.1.6",
		"polka": "1.0.0-next.11",
		"sirv": "^1.0.6",
		"yargs": "^15.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.11.5",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.5",
		"@babel/runtime": "^7.12.1",
		"focus-visible": "^5.1.0",
		"front-matter": "^3.0.2",
		"highlight.js": "^10.1.2",
		"mdsvex": "^0.8.8",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.0.5",
		"prettier-plugin-svelte": "^1.4.0",
		"rollup": "^2.28.2",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-commonjs": "^10.0.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-replace": "^2.0.0",
		"rollup-plugin-svelte": "^6.0.1",
		"rollup-plugin-terser": "^7.0.1",
		"sapper": "^0.28.3",
		"svelte": "^3.29.0"
	}
}
