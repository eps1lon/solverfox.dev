{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"packageManager": "yarn@3.2.3",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier . --ignore-path ./.eslintignore --write",
		"format:check": "prettier . --ignore-path ./.eslintignore --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"test:types": "tsc --project tsconfig.json"
	},
	"dependencies": {
		"@actions/core": "1.9.1",
		"@actions/github": "5.1.1",
		"@octokit/graphql": "5.0.1",
		"compression": "1.7.4",
		"glob": "8.0.3",
		"polka": "1.0.0-next.22",
		"sirv": "1.0.19",
		"yargs": "17.6.0"
	},
	"devDependencies": {
		"@babel/core": "7.19.3",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.19.1",
		"@babel/preset-env": "7.19.3",
		"@babel/runtime": "7.19.0",
		"focus-visible": "5.2.0",
		"front-matter": "4.0.2",
		"highlight.js": "11.6.0",
		"mdsvex": "0.10.6",
		"npm-run-all": "4.1.5",
		"prettier": "2.7.1",
		"prettier-plugin-svelte": "2.7.1",
		"rollup": "2.79.1",
		"rollup-plugin-babel": "4.4.0",
		"rollup-plugin-commonjs": "10.1.0",
		"rollup-plugin-node-resolve": "5.2.0",
		"rollup-plugin-replace": "2.2.0",
		"rollup-plugin-svelte": "7.1.0",
		"rollup-plugin-terser": "7.0.2",
		"sapper": "0.29.3",
		"svelte": "3.50.1",
		"typescript": "4.8.4"
	}
}
