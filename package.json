{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"packageManager": "yarn@4.5.3",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier . --ignore-path ./.eslintignore --write",
		"format:check": "prettier . --ignore-path ./.eslintignore --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"test:types": "tsc --project ./tsconfig.json",
		"test:types:svelte": "svelte-check --tsconfig ./src/tsconfig.json"
	},
	"dependencies": {
		"@actions/core": "1.11.1",
		"@actions/github": "6.0.0",
		"@octokit/graphql": "7.1.0",
		"compression": "1.7.5",
		"glob": "11.0.0",
		"polka": "1.0.0-next.28",
		"sirv": "3.0.0",
		"svelte-check": "^4.0.0",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@babel/core": "7.26.9",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.26.9",
		"@babel/preset-env": "7.26.9",
		"@babel/runtime": "7.26.9",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^28.0.0",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^6.0.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-typescript": "^12.0.0",
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^22.0.0",
		"focus-visible": "5.2.1",
		"front-matter": "4.0.2",
		"highlight.js": "11.10.0",
		"mdsvex": "0.12.3",
		"npm-run-all2": "7.0.1",
		"prettier": "3.4.1",
		"prettier-plugin-svelte": "3.3.3",
		"rollup": "4.28.0",
		"rollup-plugin-svelte": "7.2.2",
		"sapper": "0.29.3",
		"svelte": "3.59.2",
		"svelte-preprocess": "^6.0.0",
		"tslib": "^2.6.2",
		"typescript": "5.7.2"
	}
}
