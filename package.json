{
	"name": "solverfox.dev",
	"description": "Source for https://solverfox.dev",
	"version": "0.0.1",
	"packageManager": "yarn@3.6.3",
	"scripts": {
		"dev": "sapper dev --ext '.svx .svelte'",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy --ext '.svx .svelte'",
		"format": "prettier . --ignore-path ./.eslintignore --write",
		"format:check": "prettier . --ignore-path ./.eslintignore --check",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run",
		"test:types": "tsc --project tsconfig.json"
	},
	"dependencies": {
		"@actions/core": "1.10.0",
		"@actions/github": "5.1.1",
		"@octokit/graphql": "7.0.1",
		"compression": "1.7.4",
		"glob": "10.3.4",
		"polka": "1.0.0-next.22",
		"sirv": "1.0.19",
		"yargs": "17.7.2"
	},
	"devDependencies": {
		"@babel/core": "7.22.5",
		"@babel/plugin-syntax-dynamic-import": "7.8.3",
		"@babel/plugin-transform-runtime": "7.22.5",
		"@babel/preset-env": "7.22.5",
		"@babel/runtime": "7.22.5",
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-commonjs": "^25.0.4",
		"@rollup/plugin-node-resolve": "^15.2.1",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.4.3",
		"@types/node": "^20.5.9",
		"focus-visible": "5.2.0",
		"front-matter": "4.0.2",
		"highlight.js": "11.8.0",
		"mdsvex": "0.11.0",
		"npm-run-all": "4.1.5",
		"prettier": "3.0.3",
		"prettier-plugin-svelte": "3.0.3",
		"rollup": "3.29.0",
		"rollup-plugin-svelte": "7.1.6",
		"sapper": "0.29.3",
		"svelte": "3.59.2",
		"typescript": "5.2.2"
	}
}
